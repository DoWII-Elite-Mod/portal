<div class='p-4 md:p-8 max-w-screen-2xl lg:px-8 mx-auto relative'>
  <div class='flex justify-center items-center flex-wrap gap-4'>
    <div class='rounded-2xl p-4 bg-gray-900'>
      <div class='flex items-center'>
        <h4 class='flex-auto text-white font-bold'>Ranked Games Rating</h4>
      </div>
      <table class='table-auto whitespace-nowrap mt-4'>
        <thead>
          <tr class='text-left text-xs uppercase tracking-wider'>
            <th class='font-bold py-1 px-2 pl-0'>Mode</th>
            <th class='font-bold py-1 px-2'>Rating</th>
            <th class='font-bold py-1 px-2 pr-0'>Win Rate</th>
          </tr>
        </thead>
        <tbody>
          <tr class='text-sm'>
            <td class='p-2 pl-0'>
              <div class='flex items-center'>
                <div class='text-right pl-1 flex-auto'>1v1</div>
              </div>
            </td>
            <td class='p-2'>2152</td>
            <td class='p-2 pr-0'>
              <span class='inline-block mr-2'>84.3%</span>
              <span class='inline-block mr-2 text-green-300'>419W</span>
              <span class='inline-block text-red-300'>78L</span>
            </td>
          </tr>
          <tr class='text-sm'>
            <td class='p-2 pl-0'>
              <div class='flex items-center'>
                <div class='text-right pl-1 flex-auto'>2v2</div>
              </div>
            </td>
            <td class='p-2'>-</td>
            <td class='p-2 pr-0'>
              <span class='inline-block mr-2'>100.0%</span>
              <span class='inline-block mr-2 text-green-300'>2W</span>
              <span class='inline-block text-red-300'>0L</span>
            </td>
          </tr>
          <tr class='text-sm'>
            <td class='p-2 pl-0'>
              <div class='flex items-center'>
                <div class='text-right pl-1 flex-auto'>3v3</div>
              </div>
            </td>
            <td class='p-2'>-</td>
            <td class='p-2 pr-0'>
              <span class='inline-block mr-2'>100.0%</span>
              <span class='inline-block mr-2 text-green-300'>2W</span>
              <span class='inline-block text-red-300'>0L</span>
            </td>
          </tr>
        </tbody>
      </table>
    </div>

